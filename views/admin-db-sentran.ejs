                        <div class="col-xl-10">
                            <div class="card">
                                <div class="card-body">
                                    <h4 class="box-title">Pengiriman ke Transit </h4>
                                </div>
                                <div class="card-body--">
                                    <div class="table-stats order-table ov-h">
                                        <table class="table ">
                                            <thead>
                                                <tr>
                                                    <th class="serial">#</th>
                                                    <th>Nama Admin</th>
                                                    <th>Resi</th>
                                                    <th>Cabang Tujuan</th>
                                                    <th>Waktu</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <%items.forEach((item)=>{%>
                                                    <tr>
                                                        <td class="serial"><span class="id-column"><%= item.id_sen_tran%></span></td>
                                                        <td> <span class="name-column"><%= item.nama_admin%></span></td>
                                                        <td> <span class="name-column"><%= item.resi_barang%></span> </td>
                                                        <td> <span class="name-column"><%= item.nama_cabang_penerima%></span> </td>
                                                        <td> <span class="name-column"><%= item.waktu_penerimaan%></span> </td>
                                                    </tr>
                                                    <%})%>
                                            </tbody>
                                        </table>
